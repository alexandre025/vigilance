<% content_for :header do %>
    <%= render partial: 'shared/header', locals: {current: nil} %>
<% end %>

<%= @content.title %>
<%= @content.description %>
<%= @content.comment %>
<%= image_tag @content.image_url %>
<%= @content.created_at %>

<%= @organization.name %>

<% @content.tags.each do |t| %>
  <%= t.name %>
<% end %>


